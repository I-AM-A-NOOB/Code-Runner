{
    "debug": false,
    "filetype": {
        "python": {
            "extension": [
                "py",
                "pyw"
            ],
            "command": [
                "python -u \"$file\""
            ]
        },
        "c++": {
            "extension": [
                "cpp",
                "cxx"
            ],
            "command": [
                "clang++ \"$file\" -o \"$name.exe\"",
                ".\\\"$name.exe\""
            ]
        }
    }
}